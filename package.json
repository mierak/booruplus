{
	"name": "Lolinizer",
	"productName": "Lolinizer",
	"version": "0.1.6-ALPHA",
	"description": "My Electron application description",
	"main": ".webpack/main",
	"scripts": {
		"start": "electron-forge start",
		"debug": "@powershell -Command $env:DEBUG='electron-rebuild*,electron-forge*';electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "eslint --ext .ts,.tsx .",
		"test": "jest --verbose --maxWorkers=3",
		"coverage": "jest --coverage --silent --maxWorkers=3"
	},
	"keywords": [],
	"author": {
		"name": "mierak",
		"email": "mrkpwnz@gmail.com"
	},
	"license": "MIT",
	"config": {
		"forge": {
			"packagerConfig": {
				"asar": true,
				"icon": "./icon.ico"
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "Lolinizer",
						"loadingGif": "./chika_dance.gif",
						"iconUrl": "https://puu.sh/Ge5QV/be0a6fb81a.ico",
						"setupIcon": "./icon.ico"
					}
				}
			],
			"plugins": [
				[
					"@electron-forge/plugin-webpack",
					{
						"mainConfig": "./webpack.main.config.js",
						"renderer": {
							"config": "./webpack.renderer.config.js",
							"entryPoints": [
								{
									"html": "./src/index.html",
									"js": "./src/renderer.tsx",
									"name": "main_window",
									"preload": {
										"js": "./electron/preload.ts"
									},
									"additionalChunks": [
										"dexieworker"
									]
								}
							]
						}
					}
				]
			]
		}
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.52",
		"@electron-forge/maker-deb": "6.0.0-beta.52",
		"@electron-forge/maker-rpm": "6.0.0-beta.52",
		"@electron-forge/maker-squirrel": "6.0.0-beta.52",
		"@electron-forge/maker-zip": "6.0.0-beta.52",
		"@electron-forge/plugin-webpack": "6.0.0-beta.52",
		"@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
		"@testing-library/jest-dom": "^5.11.6",
		"@testing-library/react": "^11.2.2",
		"@testing-library/user-event": "^12.2.2",
		"@types/electron-devtools-installer": "^2.2.0",
		"@types/enzyme": "^3.10.5",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^25.2.1",
		"@types/react": "^16.9.34",
		"@types/react-dom": "^16.9.7",
		"@types/react-redux": "^7.1.7",
		"@types/redux-mock-store": "^1.0.2",
		"@types/styled-components": "^5.1.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"canvas": "^2.6.1",
		"circular-dependency-plugin": "^5.2.0",
		"copy-webpack-plugin": "^6.3.2",
		"cross-env": "^7.0.2",
		"css-loader": "^3.0.0",
		"electron": "^9.1.0",
		"electron-devtools-installer": "^3.1.1",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^7.14.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-react": "^7.19.0",
		"fake-indexeddb": "^3.0.0",
		"fork-ts-checker-webpack-plugin": "^3.1.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^26.6.3",
		"jest-fetch-mock": "^3.0.3",
		"node-loader": "^0.6.0",
		"prettier": "^2.2.1",
		"redux-devtools": "^3.5.0",
		"redux-devtools-extension": "^2.13.8",
		"redux-mock-store": "^1.5.4",
		"style-loader": "^0.23.1",
		"ts-jest": "^26.1.1",
		"ts-loader": "^6.2.1",
		"typescript": "^3.8.3",
		"worker-plugin": "^4.0.2"
	},
	"dependencies": {
		"@mierak/react-virtualized-grid": "0.0.5-ALPHA",
		"@reduxjs/toolkit": "^1.4.0",
		"@types/lodash.debounce": "^4.0.6",
		"@types/node-fetch": "^2.5.7",
		"@types/react-window": "^1.8.2",
		"@types/tar": "^4.0.3",
		"@welldone-software/why-did-you-render": "^4.3.1",
		"ant-design-pro": "^2.3.2",
		"antd": "^4.8.5",
		"comlink": "^4.3.0",
		"dexie": "3.0.0-rc.7",
		"electron-log": "^4.2.2",
		"electron-squirrel-startup": "^1.0.0",
		"eslint-plugin-react-hooks": "^4.0.8",
		"lodash.debounce": "^4.0.8",
		"moment": "^2.24.0",
		"node-fetch": "^2.6.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-redux": "^7.2.0",
		"react-window": "^1.8.5",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0",
		"styled-components": "^5.1.0",
		"tar": "^6.0.5"
	}
}
